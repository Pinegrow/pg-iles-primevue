{
  "name": "pg-iles-primevue",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://pg-iles-primevue.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-iles-primevue.git"
  },
  "scripts": {
    "dev": "iles dev",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-iles-primevue.netlify.app",
    "build": "iles build",
    "preview": "iles preview --open --port 5050",
    "type-check": "vue-tsc --noEmit",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix ./",
    "format": "prettier --write \"**/*.{html,vue,svelte,astro,js,jsx,cjs,mjs,ts,tsx,cts,mts,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "dependencies": {
    "@primevue/themes": "^4.2.5",
    "@vueuse/core": "^12.4.0",
    "defu": "^6.1.4",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.0",
    "primevue": "^4.2.5",
    "vee-validate": "^4.15.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.294",
    "@nuxt/eslint-config": "^0.7.5",
    "@pinegrow/iles-module": "^3.0.69",
    "@pinegrow/primevue-plugin": "^3.0.6",
    "@preact/preset-vite": "^2.9.4",
    "@primevue/auto-import-resolver": "^4.2.5",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@types/node": "^22.10.6",
    "@unhead/schema-org": "^1.11.16",
    "@unocss/preset-icons": "^65.4.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "iles": "^0.10.0-beta.1",
    "npm-run-all": "^4.1.5",
    "preact": "^10.25.4",
    "preact-render-to-string": "^6.5.13",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "solid-js": "^1.9.4",
    "svelte": "^5.17.4",
    "typescript": "~5.7.3",
    "unocss": "^65.4.0",
    "unplugin-auto-import": "^19.0.0",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vue-tsc": "^2.2.0"
  },
  "overrides": {
    "@sveltejs/vite-plugin-svelte": "^4.0.4"
  },
  "pnpm": {
    "overrides": {
      "@sveltejs/vite-plugin-svelte": "^4.0.4"
    }
  },
  "web-types": "web-types.json"
}
